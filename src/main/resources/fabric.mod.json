{
  "schemaVersion": 1,
  "id": "bsb",
  "version": "${version}",

  "name": "MC Blacklist Bypass",
  "description": "This mods stops the client from checking for blocked servers, which allows you, the player, to join any server that has been previously blocked.",
  "authors": [
    "svenar",
    "maxi"
  ],
  "contact": {
    "homepage": "https://svenar.nl",
    "sources": "https://github.com/svenar-nl/FabricBlockedServersBypass"
  },

  "license": "MIT",
  "icon": "assets/bsb/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "nl.svenar.noblocklist.BlockedServersBypass"
    ]
  },
  "mixins": [
    "bsb.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=${java_version}"
  },
  "suggests": {
    "another-mod": "*"
  }
}
